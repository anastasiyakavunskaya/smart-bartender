<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:padding="@dimen/fab_margin">

    <Spinner
        android:id="@+id/setting_1"
        android:layout_width="@dimen/spinner_wight"
        android:layout_height="@dimen/spinner_height"
        android:layout_gravity="center"
        />

    <Spinner
        android:id="@+id/setting_2"
        android:layout_width="@dimen/spinner_wight"
        android:layout_height="@dimen/spinner_height"
        android:layout_gravity="center"/>

    <Spinner
        android:id="@+id/setting_3"
        android:layout_width="@dimen/spinner_wight"
        android:layout_height="@dimen/spinner_height"
        android:layout_gravity="center"/>

    <Spinner
        android:id="@+id/setting_4"
        android:layout_width="@dimen/spinner_wight"
        android:layout_height="@dimen/spinner_height"
        android:layout_gravity="center"/>

    <Spinner
        android:id="@+id/setting_5"
        android:layout_width="@dimen/spinner_wight"
        android:layout_height="@dimen/spinner_height"
        android:layout_gravity="center"/>


    <Spinner
        android:id="@+id/setting_6"
        android:layout_width="@dimen/spinner_wight"
        android:layout_height="@dimen/spinner_height"
        android:layout_gravity="center"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/title_coefficient"
            android:textSize="@dimen/settings_text_size"
            android:layout_margin="@dimen/margin"
            />


    </LinearLayout>


</LinearLayout>